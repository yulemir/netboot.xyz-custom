#!ipxe

menu Centos 7
item centos7-setup CentOS 7
item 
item ipxeshell iPXE Shell
 
choose os && goto ${os}

:ipxeshell
shell
 
:centos7-setup
set base https://vault.centos.org/7.9.2009/os/x86_64/
kernel ${base}/images/pxeboot/vmlinuz initrd=initrd repo=${base} 
initrd ${base}/images/pxeboot/initrd.img 
boot || read void
