#!ipxe
###
### netboot.xyz-custom menu
###
kernel_params: initrd=initrd.magic ${cmdline}
isset ${dhcp-server} && set ipparam ip=dhcp || set ipparam ip=${ip}::${gateway}:${netmask}:::none nameserver=${dns}
set ipparam BOOTIF=${netX/mac} ${ipparam}
set install_mode inst.graphica
set repo https://vault.centos.org/7.9.2009/os/x86_64/

:custom
clear custom_choice
menu Centos
item --gap This is the first sub menu
item option_one ${space} Centos7
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:option_one
kernel ${centos_mirror}/${dir}/images/pxeboot/vmlinuz inst.repo=${repo} ${install_mode} ${params} ${ipparam} {{ kernel_params }}
initrd ${centos_mirror}/${dir}/images/pxeboot/initrd.img
boot || goto custom_exit


:custom_exit
chain utils.ipxe
exit
