#!ipxe

menu Centos 7 1.1
item centos7-setup CentOS 7
item 
item ipxeshell iPXE Shell
 
choose os && goto ${os}

:ipxeshell
shell
 
:centos7-setup
set base https://vault.centos.org/7.9.2009/os/x86_64/
kernel ${base}/images/pxeboot/vmlinuz initrd=initrd.img repo=${base} root=/dev/ram0
initrd ${base}/images/pxeboot/initrd.img  root=/dev/ram0
boot || read void
